#include <bits/stdc++.h>
using namespace std;
#define ll long long

const ll mod = 1e9+7;


int main() {

    string s;
    cin>>s;
    map<char,ll> m;
    for(ll i=0;s[i];i++){
        m[s[i]]++;
    }
    for(auto i:m){
        if(i.second%2){
            cout<<"No"<<endl;
            return 0;
        }
    }
    cout<<"Yes"<<endl;

}
