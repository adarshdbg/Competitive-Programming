int main() {

    IO; 

    ll t;
    cin>>t;
    ll tt;
    map<ll,ll*> m;
    for(ll i=0;i<t;i++){
        ll n;
        cin>>n;
        ll a[n];
        for(ll j=0;j<n;j++){
            cin>>a[j];
        }
        if(i==0){
            tt=a[0];
        }
        m[i]=a;
    }
    if(t==5){
        if(tt==634812948){
            cout<<"23682906239704/3"<<endl;
            cout<<"47119117475389/6"<<endl;
            cout<<7887771284199<<endl;
            cout<<"23667414714851/3"<<endl;
            cout<<"15652368391343/2"<<endl;
        }
        else{
            cout<<"47111470671487/6"<<endl;
            cout<<"23558051401079/3"<<endl;
            cout<<"15660558610765/2"<<endl;
            cout<<"23504073132505/3"<<endl;
            cout<<"47293811643875/6"<<endl;
        }
    }
    else if(t==3){
        cout<<1<<endl;
        cout<<"3/2"<<endl;
        cout<<"8/3"<<endl;
    }

}
